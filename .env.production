# Production Environment Variables
NODE_ENV=production
NEXT_PUBLIC_ENVIRONMENT=production

# Application Configuration
NEXT_PUBLIC_APP_NAME="Blues Dance Festival Finder"
NEXT_PUBLIC_APP_URL="https://your-domain.vercel.app"

# Database Configuration (Update with production database)
DATABASE_URL="postgresql://username:password@host:5432/database"

# NextAuth.js Configuration (Update with production values)
NEXTAUTH_SECRET="your-production-secret-key"
NEXTAUTH_URL="https://your-domain.vercel.app"

# OAuth Providers (Add production values)
GOOGLE_CLIENT_ID=""
GOOGLE_CLIENT_SECRET=""
FACEBOOK_CLIENT_ID=""
FACEBOOK_CLIENT_SECRET=""

# API Keys (Add production values)
GOOGLE_MAPS_API_KEY=""
SENDGRID_API_KEY=""

# Redis Configuration (Update with production Redis)
REDIS_URL="redis://username:password@host:port"